    "version": "0.2.0",
    "configurations": [

        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 1667 //ps aux |grep lua-language-server-go/main
        },
        {
            "name": "Launch in tcp",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["-mode","tcp","-logLevel","debug"]
        },
        {
            "name": "print AST",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/main.go",
            "args": ["-AST","testlua/test.lua"]
        }
    ]
}